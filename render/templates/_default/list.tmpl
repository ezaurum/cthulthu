{{define "contents"}}
<main>
    {{if .Error}}
        <p> {{ .Error }}</p>
    {{end}}

        <ul>
        {{ range .List }}
        <li>
            <span>{{.ID }}</span>
            <span>{{.UserName }}</span>
            <span>{{.RoleType }}</span>
        </li>
        {{ end }}
        </ul>
</main>
{{ end }}
